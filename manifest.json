{
  "name": "Cookie Fucker",
  "version": "1.0",
  "description": "Hides all cookie warnings. Light and open-sourced.",
  "content_scripts": [
   {
	"matches": ["http://*/*", "https://*/*"],
	"all_frames": true,
	"run_at": "document_end",
	"js": ["contentScript.js"]
   }
	],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
